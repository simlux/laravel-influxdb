{
    "name":        "simlux/laravel-influxdb",
    "description": "InfluxDB package for Laravel",
    "type":        "package",
    "license":     "MIT",
    "authors":     [
        {
            "name":  "Simon Kleeschulte",
            "email": "simlux78@gmail.com"
        }
    ],
    "require":     {
        "php":                   ">=7.1",
        "illuminate/log": "^5.5",
        "illuminate/config": "^5.5",
        "simlux/influxdb-php": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.3"
    },
    "autoload":    {
        "psr-4": {
            "Simlux\\LaravelInfluxDB\\":        "src",
            "Simlux\\LaravelInfluxDB\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Simlux\\LaravelInfluxDB\\LaravelInfluxDBServiceProvider"
            ]
        }
    }
}
